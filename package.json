    "egg": "^2.9.1",
    "egg-scripts": "^2.6.0",
    "egg-view-nunjucks": "^2.2.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/node": "^8.10.21",
    "egg-bin": "^4.7.1",
    "egg-ts-helper": "^1.9.0"
